    valueSetDefinitionName="NCIt Neoplasm Tree"
    defaultCodingScheme="NCI_Thesaurus" conceptDomain="Neoplastic Process">
    <lgCommon:owner>NCI</lgCommon:owner>
    <lgCommon:entityDescription><![CDATA[The full human Neoplasm sub-tree of NCI Thesaurus (NCIt), more than 8,300 concepts.  This set is updated with each monthly update of NCIt.  The released file version is <a href="http://evs.nci.nih.gov/ftp1/NCI_Thesaurus/Neoplasm/Neoplasm.zip">Neoplasm.zip</a>, a zipped OWL file that is one of the NCIt branch files.  For more details on these branch files, see the <a href="http://evs.nci.nih.gov/ftp1/NCI_Thesaurus/Branches/ReadMe.txt">ReadMe.txt</a> file.]]></lgCommon:entityDescription>
    <lgVD:mappings>
        <lgNaming:supportedCodingScheme localId="NCI_Thesaurus"
            uri="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#" isImported="true">NCI_Thesaurus</lgNaming:supportedCodingScheme>
        <lgNaming:supportedConceptDomain localId="Neoplastic Process"/>
        <lgNaming:supportedContext 
    		localId="National Cancer Institute Terminology" uri="http://evs.nci.nih.gov/valueset/C143048">
    		National Cancer Institute Terminology</lgNaming:supportedContext>
        <lgNaming:supportedNamespace localId="NCI_Thesaurus"
            uri="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#" equivalentCodingScheme="NCI_Thesaurus">NCI_Thesaurus</lgNaming:supportedNamespace>
        <lgNaming:supportedSource
            localId="NCI">NCI</lgNaming:supportedSource>
    </lgVD:mappings>
    <lgVD:source>TVS_NCI_Component</lgVD:source>
    <lgVD:representsRealmOrContext>National Cancer Institute Terminology</lgVD:representsRealmOrContext>
    <lgVD:properties/>
